def delete_occurence(lst, item):
    res=[i for i in lst if i!=item]
    return res

lst=[]
n=int(input("Enter the no.of elements : "))

for j in range(0,n):
    ele=int(input())
    lst.append(ele)

item=int(input("Enter the item to be removed : "))
print("Original list : " +str(lst))
res = delete_occurence(lst, item)
print("The list after deleting all occurrences : " +str(res))
